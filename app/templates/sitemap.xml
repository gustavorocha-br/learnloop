<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for url in urls %}
    <url>
      <loc>{{ url.loc }}</loc>
      <lastmod>{{ url.lastmod }}</lastmod>
      <changefreq>weekly</changefreq>  <!-- Frequência de mudança fixa -->
      <priority>0.7</priority>  <!-- Prioridade fixa -->
    </url>
  {% endfor %}
</urlset>
